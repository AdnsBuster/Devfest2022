{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "slots",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "slots": {
      "type": "array",
      "items": { "$ref": "#/definitions/slot" }
    }
  },
  "definitions": {
    "slot": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "key": {
          "type": "string"
        },
        "start": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "opening",
            "keynote",
            "lunch",
            "break",
            "quickie",
            "conference",
            "codelab",
            "party"
          ]
        }
      },
      "required": ["key", "start", "type"]
    }
  }
}
